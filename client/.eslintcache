Context(WalletContext);\r\n    const [transactions, setTransactions] = useState([]);\r\n    const [ dates, setDates ] = useState([])\r\n\r\n    useEffect(()=>{\r\n        const fetchData = async () => {\r\n            try{\r\n                const response = await Backend.get(`/wallets/${selectedWallet.id}`);\r\n                setTransactions(response.data.data);\r\n                setSelectedWallet({...selectedWallet, current_balance: response.data.balance.current_balance})\r\n                \r\n                const dates_temp = []\r\n                \r\n                transactions.forEach(transaction => {\r\n                    if (!dates_temp.includes(transaction.date.slice(3))){\r\n                        dates_temp.push(transaction.date.slice(3))\r\n                    }\r\n                })\r\n                setDates(dates_temp)\r\n            } catch(err) {\r\n                history.push(\"/session\");\r\n            }\r\n        }\r\n        fetchData()\r\n    },[transactions])\r\n\r\n    const handleIncome = (e) => {\r\n        e.preventDefault();\r\n        setSelectedTransaction(\"Income\")\r\n        history.push(\"/addTransaction\")\r\n    }\r\n    const handleExpense = (e) => {\r\n        e.preventDefault();\r\n        setSelectedTransaction(\"Expense\")\r\n        history.push(\"/addTransaction\")\r\n    }\r\n    const handleTransfer = (e) => {\r\n        e.preventDefault();\r\n        setSelectedTransaction(\"Transfer\")\r\n        history.push(\"/addTransaction\")\r\n    }\r\n    const handleDelete = async (e) => {\r\n        e.preventDefault();\r\n        if(window.confirm(`Are you sure you want to delete: ${selectedWallet.name}. (This cannot be undone)`)){\r\n            await Backend.delete(`/wallets/${selectedWallet.id}`);\r\n            history.push('/home');\r\n        }\r\n    }\r\n\r\n    return (\r\n        <div className=\"container\">\r\n            <button onClick={e=>history.push(\"/\")} className=\"btn btn-primary mt-1\">Logout</button>\r\n            <Header title={selectedWallet.name}/><br />\r\n            <div className=\"container\">\r\n            <div className=\"row\">\r\n                    <div className=\"col-sm\">\r\n                        <button onClick={()=>history.push(\"/home\")}className=\"btn btn-primary\">Back</button>\r\n                    </div>\r\n                    <div className=\"col-7\">\r\n                        <h3 className=\"text-left display-5\">Current Balance: ${selectedWallet.current_balance}</h3>\r\n                    </div>\r\n                    <div className=\"col-sm text-right\">\r\n                        <button onClick={()=>history.push('/editWallet')} className=\"btn btn-warning\">Edit Wallet</button>\r\n                    </div>\r\n                    <div className=\"col-sm text-right\">\r\n                        <button onClick={handleDelete} className=\"btn btn-danger\">Delete Wallet</button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            \r\n            <Tabs defaultActiveKey=\"all\" id=\"uncontrolled-tab-example\">\r\n                <Tab eventKey=\"all\" title=\"All\">\r\n                    <TransView transactions={transactions} setTransactions={setTransactions} />\r\n                </Tab>\r\n                {\r\n                    dates && dates.map(date => <Tab eventKey={date} title={date}><TransView transactions={transactions.filter(trans => trans.date.slice(3)===date)} setTransactions={setTransactions} /></Tab>) \r\n                }                       \r\n            </Tabs>\r\n\r\n            <div className=\"container\">\r\n                <div className=\"row\">\r\n                    <div className=\"col-sm text-center\">\r\n                        <button onClick={handleIncome} className=\"btn btn-success\">Add Income</button>\r\n                    </div>\r\n                    <div className=\"col-sm text-center\">\r\n                        <button onClick={handleExpense} className=\"btn btn-danger\">Add Expense</button>\r\n                    </div>\r\n                    <div className=\"col-sm text-center\">\r\n                        <button onClick={handleTransfer} className=\"btn btn-primary\">Add Transfer</button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default WalletView;","C:\\Users\\jusar\\Documents\\Websites\\MoneyTracker\\client\\src\\routes\\Register.js",[],"C:\\Users\\jusar\\Documents\\Websites\\MoneyTracker\\client\\src\\routes\\Home.js",[],"C:\\Users\\jusar\\Documents\\Websites\\MoneyTracker\\client\\src\\apis\\Backend.js",[],"C:\\Users\\jusar\\Documents\\Websites\\MoneyTracker\\client\\src\\components\\Header.js",[],"C:\\Users\\jusar\\Documents\\Websites\\MoneyTracker\\client\\src\\components\\TransView.js",[],"C:\\Users\\jusar\\Documents\\Websites\\MoneyTracker\\client\\src\\components\\main\\Wallets.js",[],"C:\\Users\\jusar\\Documents\\Websites\\MoneyTracker\\client\\src\\components\\main\\Categories.js",["96"],"C:\\Users\\jusar\\Documents\\Websites\\MoneyTracker\\client\\src\\components\\main\\Category.js",[],"C:\\Users\\jusar\\Documents\\Websites\\MoneyTracker\\client\\src\\components\\main\\Wallet.js",[],"C:\\Users\\jusar\\Documents\\Websites\\MoneyTracker\\client\\src\\components\\MonthlyAverage.js",["97"],"import React, { useContext, useEffect, useState } from 'react';\r\nimport Backend from '../apis/Backend';\r\nimport ReactFrappeChart from 'react-frappe-charts';\r\n\r\nconst MonthlyAverage = () => {\r\n    const [monthlyExpenses, setMonthlyExpenses] = useState([])\r\n    const [monthlyIncomes, setMonthlyIncomes] = useState([])\r\n\r\n    useEffect(() => {\r\n        const fetch = async () => {\r\n            try {\r\n                const response = await Backend.get('/stats/avg')\r\n                const averages = response.data.data\r\n                \r\n                const exp = averages.filter(el => el.type === 'E